{
    "name": "react-clock-app",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "node-sass": "^4.0.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.2.0",
        "react-scripts": "4.0.0"
    },
    "dependencies": {
        "moment": "^2.29.1",
        "react": "^17.0.1",
        "react-clock": "^3.0.0",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "react-hooks/exhaustive-deps": "off"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "preserve",
        "arrowParens": "avoid"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
